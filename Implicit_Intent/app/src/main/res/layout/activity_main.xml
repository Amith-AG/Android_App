<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">

    <EditText
        android:id="@+id/we"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="10dp"
        android:layout_marginBottom="10dp"
        android:ems="10"
        android:hint="Enter a web"
        android:inputType="textPersonName"
        app:layout_constraintBottom_toTopOf="@+id/wb"
        app:layout_constraintEnd_toEndOf="@+id/wb"
        app:layout_constraintHorizontal_bias="0.208"
        app:layout_constraintStart_toStartOf="@+id/wb"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/wb"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginBottom="10dp"
        android:text="Search"
        app:layout_constraintBottom_toTopOf="@+id/pe"
        app:layout_constraintEnd_toEndOf="@+id/pe"
        app:layout_constraintStart_toStartOf="@+id/pe" />

    <EditText
        android:id="@+id/pe"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginBottom="10dp"
        android:ems="10"
        android:hint="phone no"
        android:inputType="textPersonName"
        app:layout_constraintBottom_toTopOf="@+id/pb"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="@+id/pb" />

    <EditText
        android:id="@+id/le"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginBottom="10dp"
        android:ems="10"
        android:inputType="textPersonName"
        android:hint="Location"
        app:layout_constraintBottom_toTopOf="@+id/lb"
        app:layout_constraintEnd_toEndOf="@+id/lb"
        app:layout_constraintStart_toStartOf="@+id/lb" />

    <EditText
        android:id="@+id/te"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginBottom="10dp"
        android:ems="10"
        android:hint="text"
        android:inputType="textPersonName"
        app:layout_constraintBottom_toTopOf="@+id/tb"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="@+id/tb" />

    <Button
        android:id="@+id/pb"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginBottom="10dp"
        android:text="Dial"
        app:layout_constraintBottom_toTopOf="@+id/le"
        app:layout_constraintEnd_toEndOf="@+id/le"
        app:layout_constraintStart_toStartOf="@+id/le" />

    <Button
        android:id="@+id/lb"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginBottom="10dp"
        android:text="gMap"
        app:layout_constraintBottom_toTopOf="@+id/te"
        app:layout_constraintEnd_toEndOf="@+id/te"
        app:layout_constraintStart_toStartOf="@+id/te" />

    <Button
        android:id="@+id/tb"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginBottom="10dp"
        android:text="share"
        app:layout_constraintBottom_toTopOf="@+id/se"
        app:layout_constraintEnd_toEndOf="@+id/se"
        app:layout_constraintStart_toStartOf="@+id/se" />

    <EditText
        android:id="@+id/se"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginBottom="10dp"
        android:ems="10"
        android:inputType="textPersonName"
        android:hint="sms_text"
        app:layout_constraintBottom_toTopOf="@+id/sb"
        app:layout_constraintEnd_toEndOf="@+id/sb"
        app:layout_constraintStart_toStartOf="@+id/sb" />

    <Button
        android:id="@+id/sb"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginBottom="180dp"
        android:text="sms"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent" />


</androidx.constraintlayout.widget.ConstraintLayout>